<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1" />
  <title>Social App</title>
  <link rel="stylesheet" href="/css/style.css" type="text/css">
  <link rel="stylesheet" href="/css/loader.css" type="text/css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script defer src="http://localhost:3000/socket.io/socket.io.js"></script>
  <script defer type="text/javascript" src="js/client.js"></script>
  <script>
    $(window).on("load", () => {
      $(".loader-wrapper").fadeOut('slow')
    });
  </script>
</head>

<body>
  <div class="loader-wrapper" style="z-index: 500;">
    <span class="loader"><span class="loader-inner"></span></span>
  </div>
  <div class="card-app">
    <div class="card-left">
      <div class="card-profile">
        <img class="img-profile" src="https://img.icons8.com/small/100/000000/user-male-circle.png" />
        <p id="p-user-profile">
          <%= user.nickname %>
        </p>
        <form action="/logout?_method=DELETE" method="POST">
          <button type="submit">Log Out</button>
        </form>
      </div>

      <div class="card-contacts">
        <nav class="bar-top2">
          <input class="input-search" type="text" placeholder="Pesquisar..."></input>
          <button class="btn-search" onclick=""></button>
        </nav>
        <div class="card-list-contacts">
          <% users.forEach((item)=> {%>
            <div class="contact-info">
              <p style="position: absolute;left: 5%;">
                <%= item.nickname %>
              </p>
              <p style="position: absolute;right: 5%;">
                <%= item.status %>
              </p>
            </div>
            <% })%>
        </div>
      </div>
    </div>
    <div class="card-right">
      <nav class="bar-top">
        <form id="message-form">
          <textarea id="input-message" class="input-message" type="text" maxlength="250"
            placeholder="Escrever mensagem... (max 250 letras)"></textarea>
          <button class="btn-message" type="submit"></button>
        </form>
      </nav>
      <div id="message-field" class="message-field">
        <div id="message-reverse-back"> 
        </div>
      </div>
      <nav class="bar-bottom">
        <div class="message-button">
          <img class="img-center"
            src="data:image/png;base64,">
        </div>
        <div class="profile-button">
          <img class="img-center"
            src="data:image/png;base64,">
        </div>
      </nav>
    </div>
  </div>
</body>
</html>